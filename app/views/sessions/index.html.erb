<h1>Sessions#index</h1>
<div id="user-widget">
  <% if current_user %>
    Welcome <strong><%= current_user.name %></strong>!
    <%= link_to "Sign out", signout_path %>
    <%= image_tag current_user.image%>
  <% else %>
    <%= link_to "Sign in with Rdio", 'auth/rdio/' %>
  <% end %>
</div>
<p>Find me in app/views/sessions/index.html.erb</p>
