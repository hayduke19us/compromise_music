<div id="collab_title">Collaborations</div>
<div id="collab_gr_wrapper">
  <div id="collab_gr_title">Groups</div>
  <% collab_groups.each do |group|%>
    <li><%= link_to group.name, sessions_my_group_path(group: group), 
      remote: true,
      id: "gp_link" %></li>
  <%end%>
</div>
<div id="collab_pl_title">Playlists</div>
<% collab_playlists.each do |playlist|%>
  <li><%= link_to  playlist.name, 
    sessions_my_playlist_path(playlist: playlist), 
    remote: true,
    id: "pl_link" %></li>
<%end%>

