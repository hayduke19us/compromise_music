<a href="javascript:undefined" id="collab_title">Collaborations</a>
<div id='collaborations'>
  <div id="collab_gr_wrapper">
    <div id="collab_gr_title">Groups</div>
    <% user.collab_groups.each do |group|%>
      <div id="name_wrapper">
        <li><%= link_to group.name.titleize, sessions_my_group_path(group: group), 
          remote: true,
          id: "gp_link" %></li>
      </div>
    <%end%>
  </div>
  <div id="collab_pl_title">Playlists</div>
  <% user.collab_playlists.each do |playlist|%>
    <div id="name_wrapper">
      <li><%= link_to  playlist.name.titleize, 
        sessions_my_playlist_path(playlist: playlist), 
        remote: true,
        id: "pl_link" %></li>
    </div>
  <%end%>
</div>

