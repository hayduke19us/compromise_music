      <h3 id="new_playlist_name"><%=link_to @playlist.name.titleize,
        @playlist.embedUrl, :target => "_blank"%></h3>
		  <h3 id="votes">Votes</h3>
			<div id="publish">
		    <%if @user_id == @playlist_user_id%>
		      <%unless @playlist.tracks.count < 1%>
            <%= link_to 'publish', ({controller: "playlists", action: "publish"}),
              "class" => "btn", confirm: "Are you sure?" %>
		      <%end%>
		    <%end%>
		  </div>
		  <% @playlist.tracks.each do |track|%>
		    <div class= "well">
		      <h4 id="track_name">
		        <%= link_to track.name, track.embedUrl, 'id' => 'track_name'%></br>
		      </h4>
          <% unless @user_id == @playlist_user_id %> 
		        <div id="vote">
              <%=link_to raw("<i class='icon-thumbs-up'></i>"),
                controller: "tracks", action: "vote_up", id: track.id, 
			          playlist_id: @playlist.id, user_id: @playlist.user_id %>
              <%=link_to raw("<i class='icon-thumbs-down'></i>"),
                controller: "tracks", action: "vote_down", id: track.id, 
	              playlist_id: @playlist.id, user_id: @playlist.user_id %>
	          </div>
		      <%else%>
		        <div id="remove_button">
              <%=link_to "delete", track_path(track), method: "delete",
                :id => track.id %>
			      </div>
			    <%end%>
		      <div id="vote_count">
		        <h4><%=track.votes_for - track.votes_against%></h4>
		      </div></br>
		    </div>
		  <%end%>
	  </div>
